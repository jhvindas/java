    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="examen1, examen2, examen3" type="Integer" array="False" size=""/>
            <declare name="promedio" type="Integer" array="False" size=""/>
            <output expression="&quot;Digite la nota del primer examen: &quot;" newline="True"/>
            <input variable="examen1"/>
            <output expression="&quot;Digite la nota del primer examen: &quot;" newline="True"/>
            <input variable="examen2"/>
