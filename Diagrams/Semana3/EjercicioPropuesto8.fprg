            <assign variable="promedio" expression="(examen1 + examen2 + examen3) / 3"/>
            <if expression="promedio &gt;= 70">
                <then>
                    <output expression="&quot;El promedio del estudiante es de &quot; &amp; promedio &amp; &quot; por lo tanto est&#225; aprobado.&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;El promedio del estudiante es de &quot; &amp; promedio &amp; &quot; por lo tanto est&#225; reprobado.&quot;" newline="True"/>
                </else>
            </if>
        </body>
